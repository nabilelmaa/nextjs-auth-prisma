generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime
}

model Student {
  id         Int    @id @default(autoincrement())
  first_name String
  last_name  String
  major      String
}

enum Role {
  student
  professor
}
